---
import MainHead from '../components/MainHead.astro';
import Nav from '../components/Nav.astro';

let title = "About";
---
<html lang="en">
  <head>
    <MainHead
      title={title}
      canonicalURL={Astro.request.canonicalURL}
    />
  </head>
  <body class="bg-gray-900">
    <Nav title={title} />

    <main>
      <h2 class="title">{title}</h2>
      
    </main>

  </body>
</html>
